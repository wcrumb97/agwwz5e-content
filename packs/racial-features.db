{"name":"Long-Limbed","type":"feat","img":"icons/skills/melee/strike-slashes-red.webp","system":{"description":{"value":"<p>Once per turn, you can increase the range of a melee attack by 5 feet. You can use this feature a number of times between long rests equal to your proficiency bonus.</p>","chat":"","unidentified":""},"source":"Racial","activation":{"type":"none","cost":null,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value":null,"long":null,"units":""},"uses":{"value":2,"max":"@prof","per":"lr","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"Bugbear","recharge":{"value":null,"charged":false}},"effects":[],"flags":{"core":{"sourceId":"Item.lYmxfPM05qzBF379"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1671660272616,"modifiedTime":1671672570348,"lastModifiedBy":"f76cKvIj0YjCnnt9"},"_id":"lYmxfPM05qzBF379","folder":null,"sort":0,"ownership":{"default":0,"f76cKvIj0YjCnnt9":3}}
